{
  "name": "huckleberry-ogre",
  "version": "1.2.1",
  "description": "Playground personal website",
  "main": "backend/bin/runServer.js",
  "scripts": {
    "test": "npm run client & npm run server",
    "start": "run-p server client",
    "server": "npm run tsc:server && ts-node-dev --project src/server/tsconfig.json src/server/server.ts",
    "server-debug": "npm run tsc:server && ts-node-dev %NODE_DEBUG_OPTION% --project src/server/tsconfig.json src/server/server.ts",
    "client": "webpack-dev-server --mode=development",
    "webpack-error": "webpack --display-error-details",
    "tsc:server": "tsc --project src/server/tsconfig.json",
    "tsc:client": "tsc --project src/client/tsconfig.json",
    "tsc": "npm run tsc:server && npm run tsc:client"
  },
