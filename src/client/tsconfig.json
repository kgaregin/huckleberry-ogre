{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "pretty": true,
    "module": "esnext",
    "noEmit": true,
    "strict": true,
    "experimentalDecorators": true,
    "target": "ES5",
    "jsx": "react",
    "moduleResolution": "node",
    "sourceMap": true,
    "skipLibCheck": true,
    "strictPropertyInitialization": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "allowSyntheticDefaultImports": true,
    "noEmitOnError": false
  }
}